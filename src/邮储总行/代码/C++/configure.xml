<?xml version="1.0" encoding="utf-8"?>
<con>
	<!-- 章到位等待时间, 单位：秒 -->
	<wait_time>2</wait_time>
	
	<!-- 校准点, 左上, 右上(1), 右下, 左下(3), 中间 -->
	<checkpoint>
        <point0>12,34</point0>
        <point1>1914,230</point1>
        <point2>209,45</point2>
        <point3>208,1442</point3>
        <point4>101,110</point4>
	</checkpoint>
	
	<!-- 凭证摄像头图片格式, 0-JPG, 1-BMP -->
	<img_format>0</img_format>
	
	<!-- 凭证摄像头分辨率 -->
	<resolution>
		<width>2592</width>
		<height>1944</height>
	</resolution>
	
	<!-- 凭证摄像头补光灯亮度 -->
	<brightness>25</brightness>

	<!-- 用于校准印章的物理坐标,mm -->
	<physical>
		<minx>3</minx>
		<miny>60</miny>
		<maxx>270</maxx>
		<maxy>250</maxy>
	</physical>
</con>
